{
    "name": "bmunyoki/instagram",
    "description": "Laravel wrapper for Instagram Graph API. Provides functionality to get access token, user profile data and media",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Munyoki",
            "email": "munyokibenjamin@gmail.com"
        }
    ],
    "homepage": "https://github.com/bmunyoki/instagram",
    "keywords": ["Laravel", "Instagram"],
    "require": {
        "illuminate/support": "~5|~6",
        "facebook/graph-sdk": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "bmunyoki\\Instagram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bmunyoki\\Instagram\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "bmunyoki\\Instagram\\InstagramServiceProvider"
            ],
            "aliases": {
                "Instagram": "bmunyoki\\Instagram\\Facades\\Instagram"
            }
        }
    }
}